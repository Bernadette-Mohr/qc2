{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "qc2_save": {
      "type": "object",
      "properties": {
        "geometry": {
          "type": "object",
          "properties": {
            "atoms": {
              "type": ["array", "boolean"],
              "items": { "type": "string" }
            },
            "positions": {
              "type": ["array", "boolean"],
              "items": {
                "type": "array",
                "items": { "type": "number" }
              }
            }
          }
        },
        "basis": { "type": "string" },
        "multiplicity": { "type": "integer" },
        "charge": { "type": "integer" },
        "description": { "type": "string" },
        "name": { "type": "string" },
        "n_atoms": { "type": "integer" },
        "atoms": { "type": "string" },
        "protons": { "type": "integer" },
        "n_electrons": { "type": "integer" },
        "n_orbitals": { "type": ["integer", "boolean"] },
        "n_qubits": { "type": ["integer", "boolean"] },
        "nuclear_repulsion": { "type": ["number", "boolean"] },
        "hf_energy": { "type": ["number", "boolean"] },
        "canonical_orbitals": { "type": ["array", "boolean"] },
        "overlap_integrals": { "type": ["array", "boolean"] },
        "orbital_energies": { "type": ["array", "boolean"] },
        "one_body_integrals": { "type": ["array", "boolean"] },
        "two_body_integrals": { "type": ["array", "boolean"] }
      },
      "required": [
        "geometry",
        "basis",
        "multiplicity",
        "charge",
        "description",
        "name",
        "n_atoms",
        "atoms",
        "protons",
        "n_electrons"
      ]
    }
  },
  "required": ["qc2_save"],
  "root": { "$ref": "#/properties/qc2_save" }
}